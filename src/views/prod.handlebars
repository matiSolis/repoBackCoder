<div>
        <nav class="navbar navbar-expand-lg bg-body-$green-900" style="width: 100%;">
        <div class="container-fluid">
            <a class="navbar-brand">EL EMPORIO DEL JARDIN</a>
            <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
            <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
            <ul class="navbar-nav me-auto mb-2 mb-lg-0">
                <li class="nav-item">
                    <a class="nav-link active" aria-current="page" href="/">HOME</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/products">PRODUCTOS</a>
                </li>
                <li class="nav-item">
                    <a class="nav-link" href="/carts/{{user.cart}}">CARRITO</a>
                </li>
                <li class="nav-item dropdown">
                    <a class="nav-link dropdown-toggle bg-body-$green-900" href="#" role="button" data-bs-toggle="dropdown" aria-expanded="false">PERFIL</a>
                        <ul class="dropdown-menu bg-body-$green-900" style="background-color: #74ec9e; border-style: none">
                            <div>
                                <p>NOMBRE:  {{user.first_name}}</p>
                                <p>APELLIDO:  {{user.last_name}}</p>
                                <p>EMAIL:  {{user.email}}</p>
                                <li><hr class="dropdown-divider"></li>
                                <div class="dropNav">
                                    <a href="/api/session/logout"><button class="inputButton2">Cerrar sesion</button></a>
                                </div>
                            </div>
                        </ul>
                </li>
            </ul>
            <form class="d-flex" role="search" action="/products" method="get">
                <input class="form-control me-2" type="text" name="query" placeholder="Buscar producto..." aria-label="Search">
                <button class="btn btn-outline-success" type="submit">Search</button>
            </form>
            </div>
        </div>
    </nav>
    <h1 class="title">CATALOGO</h1>
    {{#if message}}
        <div class="alert alert-warning">{{message}}</div>
    {{/if}}
    <div class="gallery">
        {{#each products}}
            <div class="product">
                <h2>{{this.title}}</h2>
                <h5 class="divisorProduct"></h5>
                <img src="{{this.thumbnail}}" alt="{{this.title}}">
                <h5 class="divisorProduct"></h5>
                <p>Descripcion: {{this.description}}</p>
                <p>Precio: ${{this.price}}</p>
                <p>Stock: {{this.stock}}</p>
                <h5 class="divisorProduct"></h5>
                <div class="divBottomProducts">
                    <button class="inputButton2">Agregar al carrito</button>
                </div>
            </div>
        {{/each}}
    </div>
    <div>
        {{#if prevPage}}
            <a href="/products?{{#if urlParams.query}}query={{urlParams.query}}&{{/if}}{{#if urlParams.sort}}sort={{urlParams.sort}}&{{/if}}{{#if urlParams.category}}category={{urlParams.category}}&{{/if}}page={{prevPage}}"><button class="inputButton2">Anterior</button></a>
        {{/if}}
        {{#if nextPage}}
            <a href="/products?{{#if urlParams.query}}query={{urlParams.query}}&{{/if}}{{#if urlParams.sort}}sort={{urlParams.sort}}&{{/if}}{{#if urlParams.category}}category={{urlParams.category}}&{{/if}}page={{nextPage}}"><button class="inputButton2">Siguiente</button></a>
        {{/if}}
    </div>
</div>